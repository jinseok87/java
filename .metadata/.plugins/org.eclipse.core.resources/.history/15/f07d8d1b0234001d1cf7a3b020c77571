package sec3;

public class Loop6 {

	public static void main(String[] args) {
		int tot =0;
		for(int i=0; i<=100; i++){
			if(i%2==1) continue;
			tot+=i;
		}
		System.out.println("0~100까지 짝수의 합계 : "+tot);
		
		tot=0;
		for(int i=0; i<=100; i+=2){
			tot+=i;
		}
		System.out.println("0~100 짝수의 합계2 "+ tot);
	
	}

}
