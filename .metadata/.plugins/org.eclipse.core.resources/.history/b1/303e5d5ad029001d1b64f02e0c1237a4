package sec1;

public class Var03 {

	public static void main(String[] args) {
		boolean a=true;
		
		//정수
		byte b=20, c=127; //-128~127 -> 1byte(8bit = 2의 8승 = 256가지)
		short d=6450; //  2byte (16bit)
		int e=65000;  // 4byte(32bit)
		long f=650000; //8byte(64bit)
		//실수
		float g=3.14f;  //4byte(32bit)
		double h=3.14;  //8byte(32bit)
		//문자/문자열
		char i='k';		//2byte (16bit)
		String j="jin";
	}

}
