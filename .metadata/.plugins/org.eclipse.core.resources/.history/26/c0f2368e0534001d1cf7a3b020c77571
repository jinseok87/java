package sec3;

public class Loop7 {

	public static void main(String[] args) {
		int [][] a = {{10,20,30},{40,50,60},{70,80,90},{90,100,110}}; 
		
		for(int i=0; i<a.length; i++){
			System.out.println(a[i][0]+"\t"+a[i][1]+"\t"+a[i][2]);
		}
		System.out.println("------------------------------");
		for(int i=0; i<a.length; i++){
			for(int j=0; j<a[0].length;j++){
				System.out.println(a[i][j]+"\t");
			}
		}
	}

}
