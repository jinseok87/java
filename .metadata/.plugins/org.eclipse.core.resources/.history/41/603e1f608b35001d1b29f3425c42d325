package sec01;

public class Tyoe2 {

	public static void main(String[] args) {
		//reference (참조형) type : 여러 개(다수)의 데이터를 저장하기 위한 기억장소로서 null값이 허용됨 
		int[] arr1 = {10,20,30}	;
		int[] arr2 = {10,20,30}	;
		int[] arr3 = null;  // reference  type은 null값을 허용
//		int a = null; - primitive type(기본타입)은 null값일 수 없다.
		arr3[0] = 20;  //arr3[0] - 값 초기화 
		System.out.println(arr3[0]);
		System.out.println(arr3[1]);
	}

}
