package sec4;

public class ex6 {

	public static void main(String[] args) {

		for (int i = 1; i <= 5; i++) {
			for (int j = 1; j <= i; j++) {
				System.out.print("*");
				if (i == j) {
					System.out.println();
				}
			}
		}
		System.out.println();
		System.out.println();
		for (int a = 1; a <= 5; a++) {
			for (int b = 5; b >= 1; b--) {
				System.out.print("*");
			}
			System.out.println();
		}
	}
}
